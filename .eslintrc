{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "jest"],
	"rules": {
		"quotes": ["error", "backtick"],
		"prefer-template": "error"
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"plugin:jest/recommended",
		"plugin:jest/style"
	],
	"env": {
		"jest/globals": true,
		"node": true
	}
}
